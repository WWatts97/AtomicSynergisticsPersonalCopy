@model AtomicSynergisitcs.DATA.Models.ProductType

@{
    ViewData["Title"] = "Delete | " + Model.ProductTypeName;
}



<h3>Are you sure you want to delete @Model.ProductTypeName?</h3>
<div>
    
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.ProductTypeName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.ProductTypeName)
        </dd>
    </dl>
    
    <form asp-action="Delete">
        <input type="hidden" asp-for="ProductTypeId" />
        <input type="submit" value="Delete" class="btn btn-outline-danger" /> |
        <a asp-action="Index">Back to types of products</a>
    </form>
</div>
